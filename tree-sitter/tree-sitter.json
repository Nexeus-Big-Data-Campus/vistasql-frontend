{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sql",
      "camelcase": "Sql",
      "title": "Sql",
      "scope": "source.sql",
      "file-types": [
        "sql"
      ],
      "injection-regex": "^sql$",
      "class-name": "TreeSitterSql"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Sql grammar for tree-sitter",
    "authors": [
      {
        "name": "Jordi"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-sql"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}